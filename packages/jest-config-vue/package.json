{
  "name": "@forsakringskassan/jest-config-vue",
  "version": "29.5.7",
  "description": "Shareable jest configuration for vue applications",
  "keywords": [
    "jest"
  ],
  "homepage": "https://github.com/Forsakringskassan/jest-config",
  "bugs": "https://github.com/Forsakringskassan/jest-config/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Forsakringskassan/jest-config.git",
    "directory": "packages/jest-config-vue"
  },
  "license": "MIT",
  "author": "FÃ¶rsÃ¤kringskassan",
  "main": "jest-preset.js",
  "bin": {
    "jest": "jest.js"
  },
  "files": [
    "dist",
    "jest.js",
    "jest-preset.js"
  ],
  "scripts": {
    "build": "node build.mjs",
    "prepack": "release-prepack --bundle --retain-scripts",
    "postpack": "release-postpack",
    "prepublishOnly": "release-prepublish --bundle --retain-scripts",
    "pretest": "npm run build && node ./jest.js --version",
    "test": "run-s test:*",
    "test:vue2": "node ./jest.js --config vue2.config.js vue2-example",
    "test:vue3": "node ./jest.js --config vue3.config.js vue3-example"
  },
  "dependencies": {
    "babel-jest": "29.7.0",
    "consolidate": "1.0.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-serializer-vue": "3.1.0",
    "jest-transform-stub": "2.0.0",
    "jest-watch-typeahead": "2.2.2",
    "prettier-2": "npm:prettier@2.8.8",
    "ts-jest": "29.2.2"
  },
  "devDependencies": {
    "esbuild": "0.23.0",
    "internal-vue2-jest": "*",
    "internal-vue3-jest": "*",
    "prettier-2": "npm:prettier@2.8.8",
    "vue-template-compiler": "2.7.16",
    "vue2": "npm:vue@2.7.16",
    "vue3": "npm:vue@3.4.32"
  },
  "peerDependencies": {
    "@babel/core": "^7",
    "@vue/test-utils": "^1.3 || ^2.0",
    "vue": "^2.7 || ^3",
    "vue-template-compiler": "^2.7"
  },
  "peerDependenciesMeta": {
    "@vue/test-utils": {
      "optional": true
    },
    "vue-template-compiler": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 16.10",
    "npm": ">= 7"
  },
  "externalDependencies": [
    "babel-jest",
    "consolidate",
    "jest",
    "jest-environment-jsdom",
    "prettier-2",
    "ts-jest"
  ]
}
