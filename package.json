{
  "name": "@forsakringskassan/jest-config-monorepo",
  "version": "28.1.6",
  "private": true,
  "description": "Shareable jest configurations",
  "keywords": [
    "jest"
  ],
  "homepage": "https://github.com/Forsakringskassan/jest-config",
  "bugs": "https://github.com/Forsakringskassan/jest-config/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Forsakringskassan/jest-config.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "workspaces": [
    "internal/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npm run --workspaces --if-present build",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prepare": "husky",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "npm run --workspaces --if-present test"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "lint-staged": {
    "*.{js,json,md,css,scss,html,vue,ts}": "prettier --write",
    "*.{js,ts,vue}": "eslint --fix"
  },
  "release": {
    "extends": "@forsakringskassan/semantic-release-monorepo-config"
  },
  "devDependencies": {
    "@forsakringskassan/eslint-config": "10.2.0",
    "@forsakringskassan/eslint-config-cli": "10.2.0",
    "@forsakringskassan/eslint-config-jest": "10.2.0",
    "@forsakringskassan/eslint-config-typescript": "10.2.0",
    "@forsakringskassan/eslint-config-vue": "10.2.0",
    "@html-validate/release-scripts": "6.2.0",
    "@types/jest": "29.5.12",
    "esbuild": "0.20.1",
    "husky": "9.0.11",
    "lerna": "6.6.2",
    "lint-staged": "15.2.2",
    "npm-pkg-lint": "2.1.1",
    "npm-run-all2": "6.1.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">= 16.10",
    "npm": ">= 7"
  }
}
