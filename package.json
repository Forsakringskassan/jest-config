{
  "name": "@forsakringskassan/jest-config-monorepo",
  "version": "30.0.1",
  "private": true,
  "description": "Shareable jest configurations",
  "keywords": [
    "jest"
  ],
  "homepage": "https://github.com/Forsakringskassan/jest-config",
  "bugs": "https://github.com/Forsakringskassan/jest-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/jest-config.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "workspaces": [
    "internal/*",
    "packages/*"
  ],
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "npm run --workspaces --if-present test"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "prettier": "@forsakringskassan/prettier-config",
  "release": {
    "extends": "@forsakringskassan/semantic-release-monorepo-config"
  },
  "dependencies": {
    "prettier-2": "npm:prettier@2.8.8"
  },
  "devDependencies": {
    "@forsakringskassan/commitlint-config": "3.1.1",
    "@forsakringskassan/eslint-config": "13.0.6",
    "@forsakringskassan/eslint-config-cli": "13.0.4",
    "@forsakringskassan/eslint-config-jest": "13.0.1",
    "@forsakringskassan/eslint-config-typescript": "13.0.7",
    "@forsakringskassan/eslint-config-typescript-typeinfo": "13.0.7",
    "@forsakringskassan/prettier-config": "3.1.7",
    "@html-validate/release-scripts": "6.10.0",
    "@types/jest": "30.0.0",
    "@types/node": "20.19.19",
    "esbuild": "0.25.10",
    "lerna": "9.0.0",
    "npm-pkg-lint": "4.1.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">= 20",
    "npm": ">= 7"
  }
}
