{
  "name": "@forsakringskassan/jest-config-monorepo",
  "version": "29.6.0",
  "private": true,
  "description": "Shareable jest configurations",
  "keywords": [
    "jest"
  ],
  "homepage": "https://github.com/Forsakringskassan/jest-config",
  "bugs": "https://github.com/Forsakringskassan/jest-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/jest-config.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "workspaces": [
    "internal/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npm run --workspaces --if-present build",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prepare": "husky",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "npm run --workspaces --if-present test"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "lint-staged": {
    "*.{js,json,md,css,scss,html,vue,ts}": "prettier --write",
    "*.{js,ts,vue}": "eslint --fix"
  },
  "prettier": "@forsakringskassan/prettier-config",
  "release": {
    "extends": "@forsakringskassan/semantic-release-monorepo-config"
  },
  "dependencies": {
    "prettier-2": "npm:prettier@2.8.8"
  },
  "devDependencies": {
    "@forsakringskassan/commitlint-config": "1.4.0",
    "@forsakringskassan/eslint-config": "11.5.0",
    "@forsakringskassan/eslint-config-cli": "11.5.0",
    "@forsakringskassan/eslint-config-jest": "11.5.1",
    "@forsakringskassan/eslint-config-typescript": "11.5.1",
    "@forsakringskassan/eslint-config-vue": "11.5.1",
    "@forsakringskassan/prettier-config": "2.1.3",
    "@html-validate/release-scripts": "6.7.5",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.12",
    "esbuild": "0.24.2",
    "husky": "9.1.7",
    "lerna": "8.1.9",
    "lint-staged": "15.3.0",
    "npm-pkg-lint": "3.10.6",
    "npm-run-all2": "7.0.2",
    "typescript": "5.7.3",
    "vue": "2.7.16"
  },
  "engines": {
    "node": ">= 20",
    "npm": ">= 7"
  }
}
